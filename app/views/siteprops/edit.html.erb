<%= form_with model: @siteprop do |form| %>
    <div class="form-group">
        <%= form.label :title %>
        <%= form.text_field :title, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= form.label :landingheader %>
        <%= form.text_field :landingheader, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= form.label :landingcontent %>
        <%= form.text_area :landingcontent, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= form.label :about_header %>
        <%= form.text_field :about_header, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= form.label :about_content %>
        <%= form.text_area :about_content, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= form.label :about_image %>
        <%= form.file_field :about_image, class: 'form-control' %>
    </div>

    <div class="actions">
        <%= form.submit "Save", class: 'btn btn-primary' %>
    </div>
<% end %>
